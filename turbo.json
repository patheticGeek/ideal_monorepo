{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "prettier": {
      "outputs": []
    },
    "verify": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "prettier:fix": {
      "outputs": []
    },
    "verify:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
